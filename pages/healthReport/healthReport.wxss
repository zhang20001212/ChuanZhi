.behind {
  display: block;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}
.title {
  width: 400rpx;
  margin-top: 30rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.content {
  width: 400rpx;
  position: relative;
  margin-top: 10rpx;
  right: 10rpx;
}
.content_1{
  position: relative;
  right: 10rpx;
}

.tag {
  position: relative;
  top: 10rpx;
  margin-right: 10rpx;
}

.title_slot{
  display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
/* 体检人信息左图样式 */
.information_img {
  width: 10rpx;
  height: 50rpx;
  background: #068EE2;
  box-sizing: border-box;
  border-radius: 20rpx;
  margin-left: 5rpx;
}
/* 体检人信息文字样式 */
.information_text{
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 35rpx;
  font-weight: 300;
  margin-left: 10rpx;
}
