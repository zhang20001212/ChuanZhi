/* 体检人信息左图样式 */
.information_img {
  width: 10rpx;
  height: 50rpx;
  background: #068EE2;
  box-sizing: border-box;
  border-radius: 20rpx;
  margin-left: 20rpx;
}
/* 体检人信息文字样式 */
.information_text{
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 35rpx;
  font-weight: 300;
  margin-left: 20rpx;
}